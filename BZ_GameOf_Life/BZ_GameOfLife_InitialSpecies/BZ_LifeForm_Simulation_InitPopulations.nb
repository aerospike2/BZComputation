Cell[CellGroupData[{

Cell["Variations with initial number of species", "Section",
 CellChangeTimes->{{3.791266676197646*^9, 
  3.791266701330658*^9}},ExpressionUUID->"ad81cdcb-5d45-4d11-8bbb-\
43630960a2c7"],

Cell["\<\
In this section, we create different initial populations of life forms and \
evolve them over time. For each population number, we ran 20 experiments, to \
estimate the average properties.\
\>", "Text",
 CellChangeTimes->{{3.791266870719208*^9, 3.791266875366702*^9}, {
  3.791266935066963*^9, 
  3.791266983297696*^9}},ExpressionUUID->"3142a5ab-5353-43c6-91d4-\
fc188151d66f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"runSimulation", "[", 
   RowBox[{"nLifeForms_", ",", "nSteps_", ",", "runID_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"cnt", "=", "0"}], ",", "CAState", ",", "PWMState", ",", 
      RowBox[{"allPWMStates", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"allCAStates", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"CAState", "=", 
      RowBox[{"initCAState", "[", 
       RowBox[{"nCells", ",", "nLifeForms"}], "]"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{"Initializing", " ", "CA", " ", "States"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"PWMState", "=", 
      RowBox[{"initPWMState", "[", 
       RowBox[{"CAState", ",", "numRandom"}], "]"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{"Initializing", " ", "PWM", " ", "States"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Monitor", "[", 
      RowBox[{
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"cnt", "\[LessEqual]", " ", "nSteps"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"PWMState", "=", 
           RowBox[{"bzlifeFSM", "[", 
            RowBox[{"CAState", ",", "PWMState", ",", "numRandom"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"CAState", "=", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"updateCellState", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"i", ",", "j"}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"CAState", ",", "PWMState"}], "}"}]}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "nCells"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "nCells"}], "}"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"allPWMStates", "=", 
           RowBox[{"Join", "[", 
            RowBox[{"allPWMStates", ",", 
             RowBox[{"{", "PWMState", "}"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"allCAStates", "=", 
           RowBox[{"Join", "[", 
            RowBox[{"allCAStates", ",", 
             RowBox[{"{", "CAState", "}"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"cnt", "=", 
           RowBox[{"cnt", "+", "1"}]}]}]}], "]"}], ",", "cnt"}], "]"}], ";", 
     "\[IndentingNewLine]", 
